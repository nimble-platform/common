<xsd:schema targetNamespace="urn:moda-ml:repository:schema:TEXCatalog" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="urn:moda-ml:repository:schema:TEXCatalog" xmlns:ml="urn:moda-ml:repository:schema:TEXCatalog" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="v2013-1">

    <!-- Elemento radice   -->
    <xsd:element name="TEXCatalog" type="TEXCatalog_Type"/>

    <!-- Tipo dell'elemento radice   -->
    <xsd:complexType name="TEXCatalog_Type">
        <xsd:annotation><xsd:documentation>TEXCatalog - Textiles articles catalogue and pricelist</xsd:documentation></xsd:annotation>
        <xsd:sequence>
            <xsd:element name="TCheader" type="TCheader_Type" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="TCbody" type="TCbody_Type" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
        <xsd:attribute name="msgfunction" type="msgfunction_Type" use="optional" default="OR"/>
        <xsd:attribute name="version" type="version_Type" use="optional" default="2013-1"/>
        <xsd:attribute name="useProfile" type="useProfile_Type" use="optional"/>
    </xsd:complexType>

    <!-- Element (Instance ID: 283) -->
    <xsd:complexType name="TCheader_Type">
        <xsd:annotation><xsd:documentation>TC header - header of the document TEXTILES CATALOGUE</xsd:documentation></xsd:annotation>
        <xsd:sequence>
            <xsd:element name="msgN" type="msgN_Type" minOccurs="1" maxOccurs="1"/>
            <xsd:choice>
                <xsd:element name="msgID" type="msgID_Type" minOccurs="0" maxOccurs="1"/>
                <xsd:element name="docID" type="docID_Type" minOccurs="0" maxOccurs="1"/>
        </xsd:choice>
            <xsd:element name="msgDate" type="msgDate_Type" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="msgCurrency" type="msgCurrency_Type" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="season" type="season_Type" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="validityStart" type="validityStart_Type" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="validityEnd" type="validityEnd_Type" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="buyer" type="buyer_Type" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="supplier" type="supplier_Type" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="note" type="note_Type" minOccurs="0" maxOccurs="19"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- Element (Instance ID: 281) -->
    <xsd:complexType name="TCbody_Type">
        <xsd:annotation><xsd:documentation>TC body - body of the document TEXTILES CATALOGUE</xsd:documentation></xsd:annotation>
        <xsd:sequence>
            <xsd:element name="TCgroup" type="TCgroup_Type" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <!-- Attribute (Instance ID: 370) -->
    <xsd:simpleType name="msgfunction_Type">
        <xsd:annotation><xsd:documentation>message function - function performed by the present message with regards to the transmission</xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
<!-- Enumerazione: NT18-->            <xsd:enumeration value="CA"/>
            <xsd:enumeration value="CP"/>
            <xsd:enumeration value="OR"/>
            <xsd:enumeration value="RC"/>
            <xsd:enumeration value="RT"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- Attribute (Instance ID: 928) -->
    <xsd:simpleType name="version_Type">
        <xsd:annotation><xsd:documentation>dictionary version - Dictionary version from which the instance has been created</xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
<!-- Enumerazione: NT100-->            <xsd:enumeration value="2013-1"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- Attribute (Instance ID: 939) -->
    <xsd:simpleType name="useProfile_Type">
        <xsd:annotation><xsd:documentation>document use profile - use profile to which the present document if compliant if it was agreed by the parties. It is suggested a URI with reference to the firms or the URL of the profile.</xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
        </xsd:restriction>
    </xsd:simpleType>

    <!-- Element (Instance ID: 35) -->
    <xsd:simpleType name="msgN_Type">
        <xsd:annotation><xsd:documentation>message identifier - identification number given to the message (xml document) by its issuer</xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="35"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- Element (Instance ID: 814) -->
    <xsd:simpleType name="msgID_Type">
        <xsd:annotation><xsd:documentation>message IT identifier - key identifier of the document in the information system of the issuing Party</xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="35"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- Element (Instance ID: 374) -->
    <xsd:simpleType name="sub_docID_Type">
        <xsd:annotation><xsd:documentation>document identifier - identification number of the referenced document SINCE 2008-1 ITS USE IN THE HEADER IS DISCOURAGED (REPLACED BY msgID)</xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="80"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="docID_Type">
        <xsd:simpleContent>
          <xsd:extension base="sub_docID_Type">
            <xsd:annotation><xsd:documentation>document identifier - identification number of the referenced document SINCE 2008-1 ITS USE IN THE HEADER IS DISCOURAGED (REPLACED BY msgID)</xsd:documentation></xsd:annotation>
        <xsd:attribute name="numberingOrg" type="numberingOrg_Type" use="optional"/>
          </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <!-- Element (Instance ID: 36) -->
    <xsd:simpleType name="sub_msgDate_Type">
        <xsd:annotation><xsd:documentation>message date - date of issue of the present message (xml document), according to one of the patterns YYYY-MM-DD, YYYY-MM-DD:HH-MM or YYYY-WW.</xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="(((19|20)\d\d)-(0[1-9]|1[012])-(0[1-9]|1[0-9]|2[0-9]|3[01])(:([0,1]\d|2[0-4])-([0-5][0-9])){0,1})|(((19|20)\d\d)-([0-4]\d|[5][0-2]))"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="msgDate_Type">
        <xsd:simpleContent>
          <xsd:extension base="sub_msgDate_Type">
            <xsd:annotation><xsd:documentation>message date - date of issue of the present message (xml document), according to one of the patterns YYYY-MM-DD, YYYY-MM-DD:HH-MM or YYYY-WW.</xsd:documentation></xsd:annotation>
        <xsd:attribute name="dateForm" type="dateForm_Type" use="optional"/>
          </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

