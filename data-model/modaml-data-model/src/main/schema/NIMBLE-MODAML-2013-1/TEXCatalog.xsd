    <!-- Element (Instance ID: 47) -->
    <xsd:simpleType name="sub_id_Type">
        <xsd:annotation><xsd:documentation>Party identifier - primary identification code of a Party; it is recommended the format: nation code (ISO 3166) + VAT identification number (11 crt), with the qualifier "numberingOrg"= MF</xsd:documentation></xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="15"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="id_Type">
        <xsd:simpleContent>
          <xsd:extension base="sub_id_Type">
            <xsd:annotation><xsd:documentation>Party identifier - primary identification code of a Party; it is recommended the format: nation code (ISO 3166) + VAT identification number (11 crt), with the qualifier "numberingOrg"= MF</xsd:documentation></xsd:annotation>
        <xsd:attribute name="numberingOrg" type="numberingOrg_Type" use="optional"/>
          </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

